{
  "commands": {
    "merge": {
      "desc": "Merge parameters to template",
      "args": [
        { "name": "paramsFile", "rules": [ "notEmpty" ] },
        { "name": "templateFile", "rules": [ "notEmpty" ] }
      ],
      "examples": [
        "jazz-cli merge --params path/to/params.json --template path/to/template.jazz"
      ]
    }
  }
}

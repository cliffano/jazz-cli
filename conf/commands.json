{
  "commands": {
    "merge": {
      "desc": "Merge parameters to template",
      "args": [
        { "name": "paramsFile", "rules": [ "notEmpty" ] },
        { "name": "templateFile", "rules": [ "notEmpty" ] }
      ],
      "examples": [
        "jazz-cli merge path/to/params.json path/to/template.jazz"
      ]
    }
  }
}
